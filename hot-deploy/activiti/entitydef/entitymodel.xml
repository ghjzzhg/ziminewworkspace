    <entity entity-name="ActExtForm" package-name="org.ofbiz.activiti" title="流程扩展表-表单">
        <field name="formId" type="id-ne"></field>
        <field name="formName" type="name" not-null="true">
        </field>
        <field name="formKey" type="name" not-null="true">
            <description>表单标识，用于对应的服务的约定</description>
        </field>
        <field name="servicePrefix" type="name">
            <description>对应服务的前缀，默认为key</description>
        </field>
        <field name="category" type="name"></field>
        <field name="formType" type="name" not-null="true"></field>
        <field name="formUrl" type="long-varchar"></field>
        <field name="description" type="long-varchar"></field>
        <prim-key field="formId"/>
    </entity>
    <entity entity-name="ActExtFormContent" package-name="org.ofbiz.activiti" title="自定义表单内容">
        <field name="formId" type="id-ne"></field>
        <field name="content" type="very-long"></field>
        <prim-key field="formId"/>
        <relation type="one" fk-name="content_form" rel-entity-name="ActExtForm">
            <key-map field-name="formId"/>
        </relation>
    </entity>

    <!-- 自定义表单中的控件信息 -->
    <entity entity-name="TblActExtFormChooserInfo" package-name="org.ofbiz.activiti" title="自定义表单中的控件信息">
        <field name="id" type="id-ne"/>
        <field name="name" type="very-long"/>
        <field name="type" type="very-long"/>
        <field name="formId" type="very-long"/>
        <prim-key field="id"/>
    </entity>

    <!-- 自定义表单中的控件信息 -->
    <entity entity-name="TblActChooserInfoValue" package-name="org.ofbiz.activiti" title="自定义表单中的控件信息">
        <field name="id" type="id-ne"/>
        <field name="formId" type="id-ne"/>
        <field name="chooserId" type="id-ne"/>
        <field name="chooserKey" type="very-long"/>
        <field name="chooserValue" type="very-long"/>
        <prim-key field="id"/>
    </entity>

    <!-- 自定义表单中的控件判断的值 -->
    <entity entity-name="TblActExtFormChooserValue" package-name="org.ofbiz.activiti" title="自定义表单中的控件判断的值">
        <field name="id" type="id-ne"/>
        <field name="chooserId" type="id-ne"/>
        <field name="sourceId" type="id-ne"/>
        <field name="fromId" type="id-ne"/>
        <field name="toId" type="id-ne"/>
        <field name="operatorType" type="very-long"/>
        <field name="processDefinitionId" type="very-long"/>
        <field name="value" type="very-long"/>
        <prim-key field="id"/>
        <relation type="one" fk-name="chooser_form" rel-entity-name="TblActExtFormChooserInfo">
            <key-map field-name="chooserId" rel-field-name="id"/>
        </relation>
    </entity>

    <view-entity entity-name="searchFormChooserValue" package-name="org.ofbiz.activiti" title="自定义表单判断">
        <member-entity entity-alias="TV" entity-name="TblActExtFormChooserValue"/>
        <member-entity entity-alias="TI" entity-name="TblActExtFormChooserInfo"/>
        <alias name="chooserId" field="chooserId" entity-alias="TV"/>
        <alias name="fromId" field="fromId" entity-alias="TV"/>
        <alias name="toId" field="toId" entity-alias="TV"/>
        <alias name="operatorType" field="operatorType" entity-alias="TV"/>
        <alias name="value" field="value" entity-alias="TV"/>
        <alias name="processDefinitionId" field="processDefinitionId" entity-alias="TV"/>
        <alias name="id" field="id" entity-alias="TI"/>
        <alias name="formId" field="formId" entity-alias="TI"/>
        <alias name="name" field="name" entity-alias="TI"/>
        <alias name="type" field="type" entity-alias="TI"/>
        <view-link entity-alias="TV" rel-entity-alias="TI" rel-optional="true">
            <key-map field-name="chooserId" rel-field-name="id"/>
        </view-link>
    </view-entity>

    <entity entity-name="ActTaskExtForm" package-name="org.ofbiz.activiti" title="任务扩展表-表单">
        <field name="taskId" type="long-varchar"/>
        <field name="procDefId" type="long-varchar"/>
        <field name="formId" type="id"/>

        <prim-key field="procDefId"/>
        <prim-key field="taskId"/>
        <relation type="one" fk-name="ActTaskExtForm_Form" rel-entity-name="ActExtForm">
            <key-map field-name="formId" rel-field-name="formId"/>
        </relation>
    </entity>
    <entity entity-name="ActTaskExtUser" package-name="org.ofbiz.activiti" title="任务扩展表-执行人">
        <field name="userId" type="long-varchar"></field>
        <field name="procDefId" type="long-varchar"></field>
        <field name="taskId" type="long-varchar"></field>
        <field name="userType" type="id"></field>
