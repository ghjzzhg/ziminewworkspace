        </relation>
    </entity>

    <entity entity-name="tblActTaskAllotUser" package-name="org.ofbiz.activiti" title="执行人任务转办表">
        <field name="id" type="id-ne"><description>id</description></field>
        <field name="userId" type="long-varchar"><description>审核人</description></field>
        <field name="taskId" type="long-varchar"><description>taskId</description></field>
        <field name="allotUserId" type="id-ne"><description>转办的执行人</description></field>
        <field name="allotTime" type="date-time">发起转办时间</field>
        <prim-key field="id"/>
        <relation type="one" fk-name="PK_USERID_PARTYID" rel-entity-name="Person">
            <key-map field-name="userId" rel-field-name="partyId"/>
        </relation>
        <relation type="one" fk-name="PK_ALLOTID_PARTYID" rel-entity-name="Person">
            <key-map field-name="allotUserId" rel-field-name="partyId"/>
        </relation>
    </entity>

    <entity entity-name="ActTaskExtUserCondition" package-name="org.ofbiz.activiti" title="任务扩展表-执行人限制规则">
        <field name="userId" type="id"/>
        <field name="conditionId" type="id"/>
        <field name="groupNo" type="name"/>
        <field name="sequenceId" type="common-int"/>
        <field name="conditionType" type="name"/>
        <field name="conditionValue" type="long-varchar"/>

        <prim-key field="conditionId"/>
        <relation type="one" fk-name="ActTaskExtUserCondition_user" rel-entity-name="ActTaskExtUser">
            <key-map field-name="userId"/>
        </relation>
    </entity>

