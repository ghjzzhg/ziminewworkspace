<script type="text/javascript">
    function are_cookies_enabled()
    {
        var cookieEnabled = (navigator.cookieEnabled) ? true : false;

        if (typeof navigator.cookieEnabled == "undefined" && !cookieEnabled)
        {
            document.cookie="testcookie";
            cookieEnabled = (document.cookie.indexOf("testcookie") != -1) ? true : false;
        }
        return (cookieEnabled);
    }
    $(function(){
        if (self != top) {//会话过期后如果点击某个菜单将在iframe中显示index页面，此时需要刷新最外层页面
            top.window.location.reload();
        }
        if(!are_cookies_enabled){
            showError("浏览器需开启cookie支持，否则系统部分功能将无法正常执行");
        }

        setTimeout(changeImg, 5000);

        var error = $("#errors").val();
        if(error)
        {
            showError("<span style='color: red'>"+error+"</span>");
        }

        /*$("footer.footer").on("mouseover", function(){
            $(this).css("z-index", 1);
        }).on("mouseout", function(){
            $(this).css("z-index", -1);
        })*/

        if($.cookie("rmbUser") || $.cookie("rmbPass")){
            if ($.cookie("rmbUser") == "true") {
                $("#rememberUserName").prop("checked", true);
                $("#loginUserName").val($.cookie("username"));
            }
            if ($.cookie("rmbPass") == "true") {
                $("#rememberPassword").prop("checked", true);
                $("#loginPassword").val($.cookie("loginPassword"));
            }
        }else{
            if (amplify.store("rmbUser") == "true") {
                $("#rememberUserName").prop("checked", true);
                $("#loginUserName").val(amplify.store("username"));
            }
            if (amplify.store("rmbPass") == "true") {
                $("#rememberPassword").prop("checked", true);
                $("#loginPassword").val(amplify.store("loginPassword"));
            }
        }

        $("#loginBtn").dropdownHover();
        $(".tip").qtip({
            position: {
                my: 'top left',  // Position my top left...
                at: 'bottom right', // at the bottom right of...
            },
            style: {
                classes: 'qtip-dark qtip-bootstrap'
            }
        })
    });

    //删除cookie中的值
    function delAllCookie(){
        var myDate=new Date();
        myDate.setTime(-1000);//设置时间
        var data=document.cookie;
        var dataArray=data.split("; ");
        for(var i=0;i<dataArray.length;i++){
            var varName=dataArray[i].split("=");
            //不需要清空提示信息的cookie,且
            if(varName[0]!="showMsg") {
                document.cookie = varName[0] + "=''; expires=" + myDate.toGMTString();
            }
        }

    }

    function saveRemember() {
        var loginUserName = $("#loginUserName").val();
        var loginPassword = $("#loginPassword").val();
        if(loginUserName == null || loginUserName.trim() == "" || loginPassword.trim() == "" || loginPassword.trim() == ""){
            alert("请填写用户名密码！");
            return false;
        }
        //删除cookie,用于修复退出登录记住的账户显示不正确的bug
        delAllCookie();
        //记住用户名
        if($("#rememberUserName").is(':checked'))
        {
            var username = $("#loginUserName").val();
            amplify.store( "rmbUser", "true" );
            amplify.store( "username", username );
//            $.cookie("rmbUser", "true", { expires: 7 });
//            $.cookie("username", username, { expires: 7 });
        }else {
            amplify.store( "rmbUser", "false" );
            amplify.store( "username", "" );
//            $.cookie("rmbUser", "false", { expire: -1 });
//            $.cookie("username", "", { expires: -1 });
        }

        //记住密码
        if($("#rememberPassword").is(':checked'))
        {
            var loginPassword = $("#loginPassword").val();
            amplify.store( "rmbPass", "true" );
            amplify.store( "loginPassword", loginPassword );
//            $.cookie("rmbPass", "true", { expires: 7 });
//            $.cookie("loginPassword", loginPassword, { expires: 7 });
        }else {
            amplify.store( "rmbPass", "false" );
            amplify.store( "loginPassword", "" );
//            $.cookie("rmbPass", "false", { expire: -1 });
//            $.cookie("loginPassword", "", { expires: -1 });
        }
    }

    function changeImg() {
        var chatMessage = $("#chatMessage");
        if(!chatMessage.length){
            $("body").append('<img src="http://wpa.qq.com/pa?p=2:2881630050:53" id="chatMessage" style="z-index: 100;cursor: pointer;position:fixed;bottom:10px;left:10px;height:80px" onclick="toQQ()">');
            $("#qqMsg").append('<a href="javascript:toQQ();"><img border="0" src="http://wpa.qq.com/pa?p=2:2881630050:51" alt="点击这里给我发消息" title="点击这里给我发消息"/></a>');
        }
    }

